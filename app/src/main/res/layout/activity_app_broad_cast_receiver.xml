<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.xuweiwei.myulib.app.AppBroadCastReceiverActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="send broadcast"
        android:id="@+id/button32" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="send broadcast bc2"
        android:id="@+id/button33"
        android:layout_below="@+id/button32"
        android:layout_alignRight="@+id/button32"
        android:layout_alignEnd="@+id/button32" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sticky"
        android:id="@+id/button34"
        android:layout_alignParentStart="false"
        android:layout_below="@+id/button33"
        android:layout_alignRight="@+id/button33"
        android:layout_alignLeft="@+id/button33" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ordered"
        android:id="@+id/button35"
        android:layout_below="@+id/button34"
        android:layout_toRightOf="@+id/button34" />

</RelativeLayout>
